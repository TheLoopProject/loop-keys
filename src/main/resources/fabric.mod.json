{
  "schemaVersion": 1,
  "id": "keys",
  "version": "${version}",
  "name": "Loop Keys",
  "description": "A simple mod which adds better Keybinds to legacy Minecraft.",
  "authors": [
    "Loop",
    "Blurryface",
    "TheClashFruit"
  ],
  "contact": {
    "homepage": "https://loopmc.org",
    "sources": "https://github.com/TheLoopProject/loop-keys"
  },
  "license": "MIT",
  "icon": "assets/keys/icon.png",
  "environment": "client",
  "entrypoints": {
    "client-init": [
      "org.loopmc.keys.LoopKeys"
    ]
  },
  "mixins": [
    "keys.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}"
  }
}
